#!/usr/bin/env gene

# Simple LLM Inference Example
# A minimal demonstration of AI functions in Gene

(println "=== Simple LLM Demo ===")
(println)

# Create AI components
(var tok (tokenizer/create 50000))
(println "Tokenizer:" tok)

(var embed (embedding/create 768))
(println "Embeddings:" embed)

(var mdl (model/create "simple_model" "gene"))
(println "Model:" mdl)

# Create tensors
(var input (tensor/zeros [1 10]))
(println "Input tensor:" input)

(var output (tensor/create [1 50000] :float32))
(println "Output tensor:" output)

(println)
(println "=== Demo Complete ===")
true